generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User{
  id Int @default(autoincrement()) @id
  username String @unique
  password String
  age Int 
  city String?

}

model todos{
  id Int @default(autoincrement()) @id
  title String
  desc String
  done Boolean
  userId Int
  time DateTime @default(now())
}